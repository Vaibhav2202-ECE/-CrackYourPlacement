 int maximumProduct(vector<int>& nums) {
        int n=nums.size();
        sort(nums.begin(),nums.end());
        int maxi=INT_MIN;
        int product1=nums[n-3]*nums[n-2]*nums[n-1];
        int product2=nums[0]*nums[1]*nums[n-1];
        maxi=max(product1,product2);
        return maxi;
    }